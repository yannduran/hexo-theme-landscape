<footer id="footer">
    <% if (theme.sidebar === 'bottom'){ %>
    <%- partial('_partial/sidebar') %>
    <% } %>
    <div class="outer">
        <div id="footer-info" class="inner">
            <img src=url(logo-url) />
            <br>
            &copy; <%= date(new Date(), 'YYYY') %> <a href="<%= config.author_website %>" target="_blank"><%= config.author || config.title %></a>
            <br>
            <%= __('powered_by') %> <a href="http://hexo.io/" target="_blank">Hexo</a>
        </div>
    </div>
</footer>